<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <ApplicationIcon />
    <OutputType>Library</OutputType>
    <StartupObject />
    <AssemblyName>Bve5_Parsing</AssemblyName>
    <Description>BveTrainsim5.7 Syntax parsing library.</Description>
    <Copyright>Copyright Â© 2017 aoisupersix</Copyright>
    <PackageLicenseUrl>https://github.com/aoisupersix/Bve5_Parsing/blob/master/License.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/aoisupersix/Bve5_Parsing</PackageProjectUrl>
    <RepositoryUrl>https://github.com/aoisupersix/Bve5_Parsing</RepositoryUrl>
    <AssemblyVersion>0.6.0.0</AssemblyVersion>
    <FileVersion>0.6.0.0</FileVersion>
    <Version>0.6.0</Version>
    <Company />
    <Authors>aoisupersix</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Antlr4" Version="4.6.5" />
  </ItemGroup>

  <ItemGroup>
    <Antlr4 Update="MapGrammar\ANTLR_SyntaxDefinitions\MapGrammarLexer.g4">
      <CustomToolNamespace>Bve5_Parsing.MapGrammar.SyntaxDefinitions</CustomToolNamespace>
    </Antlr4>
    <Antlr4 Update="MapGrammar\ANTLR_SyntaxDefinitions\MapGrammarParser.g4">
      <CustomToolNamespace>Bve5_Parsing.MapGrammar.SyntaxDefinitions</CustomToolNamespace>
    </Antlr4>
    <Antlr4 Update="ScenarioGrammar\ANTLR_SyntaxDefinitions\ScenarioGrammarLexer.g4">
      <CustomToolNamespace>Bve5_Parsing.ScenarioGrammar.SyntaxDefinitions</CustomToolNamespace>
    </Antlr4>
    <Antlr4 Update="ScenarioGrammar\ANTLR_SyntaxDefinitions\ScenarioGrammarParser.g4">
      <CustomToolNamespace>Bve5_Parsing.ScenarioGrammar.SyntaxDefinitions</CustomToolNamespace>
    </Antlr4>
  </ItemGroup>

</Project>
